.full-width
  width 100%
.center-content
  display flex
  justify-content center
  align-items center
.row
    width 100%
.clearfix
     margin-left -15px
     margin-right -15px

.menu ul
    display flex
    line-height 65px
    margin 0
.menu ul li
    list-style none
    margin-right 13px
.menu ul li a
    padding  10px 30px 10px 30px
    color black
    border-radius 23px
    border 1px solid #62728c
    text-decoration none
    font-family monospace
    font-size 15px
    transition 0.3s ease
.menu ul li a:hover
    background #62728c
    color white

.padd-top-55
    padding-top 55px;
.margin-top-15
    margin-top 15px;
.text-align-center
    text-align center
.display_flex-center
    display flex
    justify-content center
.float-right
    float right
.float-left
    float left
.padd-0
    padding 0
.margin-top-20
  margin-top 20px