.full-width
  width 100%
.center-content
  display flex
  justify-content center
  align-items center
.row
    width 100%
.clearfix
     margin-left -15px
     margin-right -15px

.menu
   background #1b2242
.menu ul
    display flex
    line-height 65px
    margin 0
.menu ul li
    list-style none
.menu ul li a
    padding  25px 23px 24px 23px
    color white
    text-decoration none
    transition 0.3s ease
.menu ul li a:hover
    background #273a91

.padd-top-55
    padding-top 55px;
.margin-top-15
    margin-top 15px